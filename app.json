{
  "expo": {
    "name": "ecommerce",
    "slug": "ecommerce",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.ecommerce"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.ecommerce"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter_18pt-Regular.ttf",
            "./assets/fonts/Inter_18pt-SemiBold.ttf",
            "./assets/fonts/Inter_18pt-Medium.ttf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "Allow ecommerce to access your camera",
          "microphonePermission": "Allow ecommerce to access your microphone",
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "recordAudioAndroid": true
        }
      ]
    ]
  }
}
